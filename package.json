{
  "name": "dbusjs",
  "version": "1.0.0",
  "description": "A pure JS implementation of dbus",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build && npm run generate-dts",
    "build": "webpack --mode=production",
    "watch": "webpack --mode=development --watch",
    "generate-dts": "tsc --declaration --emitDeclarationOnly",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unreadable-code/dbusjs.git"
  },
  "keywords": [
    "dbus"
  ],
  "author": "UnreadableCode",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/unreadable-code/dbusjs/issues"
  },
  "homepage": "https://github.com/unreadable-code/dbusjs#readme",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "babel-loader": "^9.1.3",
    "eslint": "^8.18.0",
    "eslint-webpack-plugin": "^4.0.1",
    "jest-environment-jsdom": "^29.5.0",
    "license-webpack-plugin": "^4.0.2",
    "terser-webpack-plugin": "^5.3.3",
    "typescript": "^5.3.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^5.1.4",
    "webpack-config-builder": "github:unreadable-code/webpack-config-builder"
  },
  "dependencies": {
    "fast-xml-parser": "^4.3.5"
  }
}
